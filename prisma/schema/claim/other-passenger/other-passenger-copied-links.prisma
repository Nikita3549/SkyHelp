model OtherPassengerCopiedLink {
  id               String          @id @default(cuid()) @map("id")
  openedAt         DateTime?       @map("opened_at")
  isSent           Boolean         @default(false)
  otherPassengerId String?         @map("other_passenger_id")
  otherPassenger   OtherPassenger? @relation(fields: [otherPassengerId], references: [id], onDelete: Cascade)

  @@unique([otherPassengerId])
  @@map("other_passenger_copied_links")
}
